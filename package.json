{
  "name": "landing",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "element-ui": "^2.1.0",
    "node-sass": "^4.7.2",
    "vue": "^2.5.13",
    "vue-clipboard2": "^0.1.0",
    "vue-i18n": "^7.6.0",
    "vue-resource": "^1.5.0",
    "vue-router": "^3.0.1",
    "vue-scroll": "^2.1.7",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.0-alpha.11",
    "@vue/cli-plugin-eslint": "^3.0.0-alpha.11",
    "@vue/cli-service": "^3.0.0-alpha.11",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-plugin-html": "^4.0.2",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^6.0.0",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "sass-loader": "^6.0.6",
    "vue-template-compiler": "^2.5.13"
  },
  "babel": {
    "presets": [
      "@vue/app"
    ]
  },
  "eslintConfig": {
    "root": true,
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "env": {
      "browser": true
    },
    "extends": "standard",
    "plugins": [
      "html"
    ],
    "globals": {
      "utils": true
    },
    "rules": {
      "semi": [
        2,
        "always",
        {
          "omitLastInOneLineBlock": true
        }
      ],
      "arrow-parens": 0,
      "generator-star-spacing": 0,
      "prefer-const": [
        "error",
        {
          "destructuring": "any",
          "ignoreReadBeforeAssign": false
        }
      ]
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
