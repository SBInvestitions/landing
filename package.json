{
  "name": "landing",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "babel-core": "^7.0.0-bridge.0",
    "babel-loader": "^7.1.4",
    "cache-loader": "^1.2.2",
    "cjs": "0.0.11",
    "driver.js": "^0.3.8",
    "element-ui": "^2.4.6",
    "ethjs-contract": "^0.2.3",
    "ethjs-query": "^0.3.8",
    "i": "^0.3.6",
    "moment": "^2.22.1",
    "node-gyp": "^3.6.2",
    "node-sass": "^4.8.3",
    "npm": "^6.4.0",
    "path": "^0.12.7",
    "pdfjs-dist": "^2.0.480",
    "vue": "^2.5.13",
    "vue-clipboard2": "^0.1.1",
    "vue-i18n": "^7.6.0",
    "vue-image-loader": "^1.0.4",
    "vue-loader": "^14.2.2",
    "vue-moment": "^4.0.0",
    "vue-pdf": "^3.2.2",
    "vue-resource": "^1.5.0",
    "vue-router": "^3.0.1",
    "vue-scroll": "^2.1.7",
    "vue-social-sharing": "^2.3.3",
    "vue2-editor": "^2.6.6",
    "vuex": "^3.0.1",
    "web3": "^0.2.8"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.0-alpha.11",
    "@vue/cli-plugin-eslint": "^3.0.0-alpha.11",
    "@vue/cli-service": "^3.0.0-alpha.11",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-plugin-html": "^4.0.2",
    "eslint-plugin-import": "^2.10.0",
    "eslint-plugin-node": "^6.0.0",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "sass-loader": "^6.0.6",
    "vue-lazyload": "^1.2.3",
    "vue-template-compiler": "^2.5.13"
  },
  "babel": {
    "presets": [
      "@vue/app"
    ]
  },
  "eslintConfig": {
    "root": true,
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "env": {
      "browser": true
    },
    "extends": "standard",
    "plugins": [
      "html"
    ],
    "globals": {
      "utils": true
    },
    "rules": {
      "semi": [
        2,
        "always",
        {
          "omitLastInOneLineBlock": true
        }
      ],
      "arrow-parens": 0,
      "generator-star-spacing": 0,
      "prefer-const": [
        "error",
        {
          "destructuring": "any",
          "ignoreReadBeforeAssign": false
        }
      ]
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
